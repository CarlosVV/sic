@using Nagnoi.SiC.Web.Models.PreExisting

@model PreExistingViewModel
@{
    ViewBag.Title = "Case";
}


@section Styles{
    @Styles.Render("~/bundles/datatables/css")
}


@section Scripts {
    @Scripts.Render("~/bundles/datatables")
    @Scripts.Render("~/bundles/Preexisting")
    @Scripts.Render("~/bundles/SearchCase")
}

<div class="homePage">
    <div class="row">
        <div class="col-md-12">
            <div class="page-header text-danger">
                <h1>Casos Pre-Existentes</h1>
            </div>
        </div>
    </div>

    @*<div class="row">
            <div class="col-md-12">
                <div class="jumbotron">

                </div>
            </div>
        </div>*@

    @*@Html.Partial("_HeaderCase", Model.CaseModel)*@

    @*
        Panel Total Adjudicado
    *@
    <div class="panel panel-default" id="CaseBalance">
        <div class="panel-heading" role="tab" id="heading2">
            <h4 class="panel-title">
                <a role="button" data-toggle="collapse" href="#link2" aria-expanded="true" aria-controls="collapse3">
                    <strong class="text-danger">Total Adjudicado</strong>
                </a>
            </h4>
        </div>
        <div id="link2" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading2">
            <div class="panel-body">
                <div class="row">

                    <div class="col-md-3">
                        <div class="form-group">
                            <label class="control-label" for="">En este caso</label>
                            <input class="form-control" name="" type="number" maxlength="75" readonly value="" />
                        </div>
                    </div>

                    <div class="col-md-1"></div>

                    <div class="col-md-3">
                        <div class="form-group">
                            <label class="control-label" for="">En casos anteriores</label>
                            <input class="form-control" name="" type="number" maxlength="75" readonly />
                        </div>
                    </div>

                    <div class="col-md-1"></div>

                    <div class="col-md-3">
                        <div class="form-group">
                            <label class="control-label" for="">Lesionado</label>
                            <input class="form-control" name="" type="number" maxlength="75" readonly />
                        </div>
                    </div>
                    <div class="col-md-1"></div>
                </div>
            </div>
        </div>
    </div>


    @*
        Panel Casos Pre-Existentes
    *@
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default" id="PreexistingCases">
                <div class="panel-heading" role="tab" id="heading2">
                    <h4 class="panel-title">
                        <a role="button" data-toggle="collapse" href="#link3" aria-expanded="true" aria-controls="collapse4">
                            <strong class="text-danger">Casos Pre-Existentes</strong>
                        </a>
                    </h4>
                </div>
                <div id="link3" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading2">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class='table-responsive'>

                                    <div>

                                        <!-- Nav tabs -->
                                        <ul class="nav nav-tabs" role="tablist">
                                            <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">Por Número de Caso</a></li>
                                            <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">Por Región Anatómica</a></li>
                                        </ul>

                                        <!-- Tab panes -->
                                        <div class="tab-content">
                                            <div role="tabpanel" class="tab-pane active" id="home">

                                                <table id="CasesTable" class="table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr class="active">
                                                            <th></th>
                                                            <th>Número de Caso</th>
                                                            <th>Nombre</th>
                                                            <th>Seguro Social</th>
                                                            <th>Total Adjudicado</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                    <tfoot></tfoot>
                                                </table>

                                            </div>
                                            <div role="tabpanel" class="tab-pane" id="profile">

                                                <table id="CasesTable2" class="table table-striped table-bordered table-hover">
                                                    <thead>
                                                        <tr class="active">
                                                            <th>Número de Caso</th>
                                                            <th>Nombre</th>
                                                            <th>Seguro Social</th>
                                                            <th>Total Adjudicado</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody></tbody>
                                                    <tfoot></tfoot>
                                                </table>

                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-default" id="OtherPreexistingCases">
                <div class="panel-heading" role="tab" id="heading2">
                    <h4 class="panel-title">
                        <a role="button" data-toggle="collapse" href="#link4" aria-expanded="true" aria-controls="collapse4">
                            <strong class="text-danger">Sugerencia de otros posibles casos relacionados</strong>
                        </a>
                    </h4>
                </div>
                <div id="link4" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="heading2">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class='table-responsive'>

                                    <div>

                                        <table id="OtherCasesTable" class="table table-striped table-bordered table-hover">
                                            <thead>
                                                <tr class="active">
                                                    <th></th>
                                                    <th>Número de Caso</th>
                                                    <th>Nombre</th>
                                                    <th>Seguro Social</th>
                                                    <th>Total Adjudicado</th>
                                                </tr>
                                            </thead>
                                            <tbody></tbody>
                                            <tfoot></tfoot>
                                        </table>


                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row">

                            @*<div class="col-md-offset-3 col-md-6">
                                <div class="form-group">
                                    <label class="control-label" for="searchOtherBtn"></label>
                                    <div class="btn-group btn-block" data-toggle="modal" data-target="#searchModal">
                                        <button class="btn btn-lg btn-info btn-block" id="searchOtherBtn">Buscar otros casos relacionados</button>
                                    </div>
                                </div>
                            </div>*@
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    @Html.Partial("SearchCase/_Modal")

    @Html.Partial("_EditTransactionDetail")
</div>