<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default">
            <div class="panel-heading">
                <span class="panel-title">
                    <strong class="text-danger">
                        Lista de Casos
                    </strong>
                </span>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class='table-responsive'>
                            <table id="tblLawyers" class="table table-striped table-bordered table-hover">
                                <thead>
                                    <tr class="active">
                                        <th>
                                            @Model.ResourceService.GetResourceString("FormAprobaciones.NumeroDeCaso.header.text")
                                        </th>
                                        <th></th>
                                        <th>
                                            @Model.ResourceService.GetResourceString("FormAprobaciones.NombreLesionado.header.text")
                                        </th>
                                        <th>
                                            SSN
                                        </th>
                                        <th>
                                            Fecha Decisión
                                        </th>
                                        <th>
                                            Abogado
                                        </th>
                                        <th>
                                            Fecha Notificación
                                        </th>
                                        <th>
                                            Número de Caso CI
                                        </th>
                                        <th>
                                            Monto a Pagar
                                        </th>
                                        <th>
                                            @Model.ResourceService.GetResourceString("FormAprobaciones.Estado.header.text")
                                        </th>
                                        <th></th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                                <tfoot></tfoot>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div id="modal-lawyer" class="modal fade" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title">
                    <strong class="text-danger">
                        Editar Honorario de Abogados
                    </strong>
                </h4>
            </div>
            <div class="modal-body">
                <input id="paymentlawyerid" name="paymentlawyerid" type="hidden" />
                <input id="transactionlawyerid" name="transactionlawyerid" type="hidden" />
                <div class="panel-body">
                    <div id="msgerror" class="alert alert-danger hidden"></div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_fechadecision">
                                    @Model.ResourceService.GetResourceString("PagoDietas.FechaDecision.label.text")
                                </label>
                                <div class='input-group date'>
                                    <input id="lawyer_fechadecision" name="lawyer_fechadecision" type="text" class="form-control" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <h5>
                                <strong class="text-danger">
                                    Información de Comisión Industrial
                                </strong>
                            </h5>
                            <hr />
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_fvisita">
                                    @Model.ResourceService.GetResourceString("PagoDietas.FechaVisita.label.text")
                                </label>
                                <div class='input-group date'>
                                    <input id="lawyer_fvisita" name="lawyer_fvisita" type="text" class="form-control" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_fnotificacion">
                                    @Model.ResourceService.GetResourceString("PagoDietas.FechaNotificacion.label.text")
                                </label>
                                <div class='input-group date'>
                                    <input id="lawyer_fnotificacion" name="lawyer_fnotificacion" type="text" class="form-control" />
                                    <span class="input-group-addon">
                                        <span class="glyphicon glyphicon-calendar"></span>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_numerocaso">
                                    @Model.ResourceService.GetResourceString("PagoDietas.NumeroCaso.label.text")
                                </label>
                                <input id="lawyer_numerocaso" name="lawyer_numerocaso" class="form-control" type="text" maxlength="75" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-4">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_montototal">
                                    @Model.ResourceService.GetResourceString("PagoDietas.MontoTotal.label.text")
                                </label>
                                <input id="lawyer_montototal" name="lawyer_montototal" class="form-control" type="text" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="control-label" for="lawyer_observaciones">
                                    @Model.ResourceService.GetResourceString("PagoDietas.Observaciones.label.text")
                                </label>
                                <textarea id="lawyer_observaciones" name="lawyer_observaciones" class="form-control" rows="5" required></textarea>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btnUpdateLawyer" type="button" class="btn btn-info">
                    Actualizar
                </button>
                <button type="button" class="btn btn-warning" data-dismiss="modal">
                    Cancelar
                </button>
            </div>
        </div>
    </div>
</div>