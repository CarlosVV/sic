@model Nagnoi.SiC.Web.Models.Case.SearchViewModel

<div class="row">
    <div class="col-md-12">
        <div class="panel panel-default" id="searchPanel">
            <div class="panel-heading">
                <span class="panel-title">
                    <strong class="text-danger">Búsqueda de caso</strong>
                    <a id="clearBtn" class="btn btn-warning btn-xs pull-right"><span class="fa fa-eraser"></span></a>
                </span>
            </div>
            <div class="panel-body">
                @Html.Partial("SearchCase/_Form")
                <div class="row">
                    <div class="col-md-12">
                        <button class="btn btn-lg btn-info pull-right" id="searchBtn">Buscar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("SearchCase/_ResultBox")